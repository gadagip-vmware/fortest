{{!
  author: samueldoyle
  model: model/importForm
}}
<div class="offset1 span6">
    <form class="form-horizontal" id="importForm">
        <div class="control-group">
            <div class="x-form-item">
                <label class="x-form-item-label-top" for="appDirHost">
                    {{hostLabel}}
                    <span class="x-darwin-required-field">*</span>
                </label>

                <div class="x-form-item-body">
                    <input type="text" id="appDirHost" disabled="" placeholder="{{appDirHost}}" value="{{appDirHost}}"
                           class="x-form-text"
                           style="width: 380px;">
                </div>
            </div>
        </div>
        <div class="control-group">
            <div class="x-form-item">
                <label class="x-form-item-label-top" for="appDirTenant">
                    {{tenantLabel}}
                    <span class="x-darwin-required-field">*</span>
                </label>

                <div class="x-form-item-body">
                    <input type="text" id="appDirTenant" disabled="" placeholder="{{appDirTenant}}"
                           value="{{appDirTenant}}" class="x-form-text"
                           style="width: 380px;">
                </div>
            </div>
        </div>
        <div class="control-group">
            <div class="x-form-item">
                <label class="x-form-item-label-top" for="appDirBusinessGroups">
                    {{businessGroupLabel}}
                    <span class="x-darwin-required-field">*</span>
                </label>

                <div class="x-form-item-body">
                    <select id="appDirBusinessGroups">
                        {{#businessGroupCollection}}
                            <option value="{{name}}">{{name}}</option>
                        {{/businessGroupCollection}}
                    </select>
                </div>
            </div>
        </div>

        <div class="control-group">
            <div class="x-form-item">
                <label class="x-form-item-label-top" for="bpExportFN">
                    {{fileLabel}}
                    <span class="x-darwin-required-field">*</span>
                </label>

                <div class="x-form-item-body">
                    <input type="text" id="bpExportFN" disabled="" class="x-form-text"
                           style="width: 380px;">
                </div>
            </div>
        </div>

        <div id="bottomWrapper">
            <hr style="border-width:2px"/>
            <p class="text-info text-left">
                {{authPromptText}}
            </p>

            <div id="actionButtons" class="control-group" style="margin-top: 30px;">
                <div id="loginButton">
                    <button type="submit" class="btn btn-primary btn-lg"
                            style="float: left">{{loginButtonText}}</button>
                </div>
                <div id="viewImportFileButton">
                    <button type="button" class="btn btn-primary btn-lg">{{downloadButtonText}}</button>
                </div>
            </div>
        </div>
    </form>
</div>
